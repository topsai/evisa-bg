/*-------左上角按钮-------*/
.canui-head-box{
    position:fixed;
    top:26px;
    margin-left:10px;
    background:rgba(0,0,0,0.2);
    color:#fff;
    border-radius:20px;
    padding:4px 4px;
    border:1px solid rgba(255,255,255,0.3);
}
.canui-head-box navigator{
    padding:8px;
    font-size:20px;
    display:initial;
}
.canui-head-box navigator + navigator{
    border-left:1px solid rgba(255,255,255,0.3);
    margin:0 3px;
    padding:0 5px;
    padding-left:15px;
}

.canui-goods-box{
